services:
  session:
    ports:
      - "10000:10000/tcp"
      - "10000:10000/udp"
      - "10002:10002/tcp"
      - "10003:10003/tcp"
    environment:
      - SERVICE_NODE_IP_ADDRESS=
      - L2_PROVIDER=
      - LOKINET_BIND_IP=
      - LMQ_PORT=10000
      - STORAGE_SERVER_PORT=10001
      - P2P_PORT=10002
      - QUORUMNET_PORT=10003
  session-storage:
    ports:
      - "10001:10001/tcp"
    environment:
      - LMQ_PORT=10000
      - STORAGE_SERVER_PORT=10001
  session-lokinet:
    ports:
      - "10004:10004/udp"
    environment:
      - SERVICE_NODE_IP_ADDRESS=
      - LOKINET_PORT=10004
      - LOKINET_BIND_IP=